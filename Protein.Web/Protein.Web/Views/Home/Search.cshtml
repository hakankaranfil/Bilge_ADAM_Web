@model IList<Protein.Entity.Model.Product>
@{
    ViewBag.Title = "Search";
}

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-4 col-lg-2 col-md-4">
            <div class="img-responsive">
                <img src="@item.ProductPhoto" height="200" width="200" alt="">
                <h4>
                    <a href="/Home/ProductDetails/@item.ProductID">@item.ProductName</a>
                </h4>
                <p class="product-price">@item.Price TL</p>


            </div>
        </div>
    }
</div>



